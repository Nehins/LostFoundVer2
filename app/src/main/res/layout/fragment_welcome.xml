<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/fondowelcome_opt"
        tools:context=".Welcome">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="194dp"
            android:layout_height="197dp"
            android:layout_marginTop="60dp"
            android:layout_marginEnd="109dp"
            android:background="#00FF0000"
            android:src="@drawable/titulo"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/iniciSessiobutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="77dp"
            android:layout_marginEnd="82dp"
            android:background="@null"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView"
            app:srcCompat="@drawable/botoinici" />

        <ImageButton
            android:id="@+id/RegistreSessiobutton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="37dp"
            android:layout_marginEnd="94dp"
            android:background="@null"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/iniciSessiobutton"
            app:srcCompat="@drawable/botoresgistre" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>